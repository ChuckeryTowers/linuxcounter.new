{% trans_default_domain 'FOSUserBundle' %}

<form action="{{ path('syw_front_main_info_edit') }}" {{ form_enctype(form) }} method="POST" class="fos_user_profile_edit">
    {{ form_row(form.firstname) }}
    {{ form_row(form.city) }}
    <div>
        <input type="submit" value="{{ 'profile.edit.submit'|trans }}" />
    </div>
</form>
